{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/auth/(.*)",
            "dest": "/routes/userAuth.js"
        },
        {
            "src": "/v1/(.*)",
            "dest": "/routes/usersCrud.js"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}